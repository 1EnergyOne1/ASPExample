@page
@model EnergyOne.Pages.SecName
<div class="row">
    <h1 class="display-2">Имена</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>People`s Name</th></tr>
        </thead>
        <tbody>
            @foreach (string name in Model.Names)
            {
                <tr><td>@name</td></tr>
            }
        </tbody>
    </table>
</div>